<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/item_titlebar"></include>

    <include layout="@layout/item_search"></include>

    <LinearLayout
        android:id="@+id/ll_nurseStage"
        android:layout_width="match_parent"
        android:layout_height="@dimen/one_finger_width"
        android:layout_marginTop="5dp"
        android:background="@drawable/item_selector"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5dp">

        <TextView

            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_left_iv_user"
            android:layout_weight="1"
            android:text="执行医嘱"
            android:textColor="@color/myPationte_title_color"
            android:textSize="16dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_left_iv_user"
            android:background="@mipmap/oh" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/big_bg_color" />

    <LinearLayout
        android:id="@+id/ll_nurseHistory"
        android:layout_width="match_parent"
        android:layout_height="@dimen/one_finger_width"
        android:background="@drawable/item_selector"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="2dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_left_iv_user"
            android:layout_weight="1"
            android:text="护理记录"
            android:textColor="@color/myPationte_title_color"
            android:textSize="16dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_left_iv_user"
            android:background="@mipmap/oh" />

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/big_bg_color" />

    <LinearLayout
        android:id="@+id/ll_nursingContentStage"
        android:layout_width="match_parent"
        android:layout_height="@dimen/one_finger_width"
        android:background="@drawable/item_selector"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="2dp">

        <TextView

            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_left_iv_user"
            android:layout_weight="1"
            android:text="执行护理"
            android:textColor="@color/myPationte_title_color"
            android:textSize="16dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_left_iv_user"
            android:background="@mipmap/oh" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/big_bg_color" />
    <View
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="@color/big_bg_color" />

    <LinearLayout
        android:id="@+id/ll_addNursingCare"
        android:layout_width="match_parent"
        android:layout_height="@dimen/one_finger_width"
        android:background="@drawable/item_selector"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5dp"
        android:paddingTop="2dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_left_iv_user"
            android:layout_weight="1"
            android:text="新增护理"
            android:textColor="@color/myPationte_title_color"
            android:textSize="16dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_left_iv_user"
            android:background="@mipmap/oh" />

    </LinearLayout>

